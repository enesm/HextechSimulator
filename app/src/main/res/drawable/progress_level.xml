<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/progress">
        <shape
            android:innerRadiusRatio="2.95"
            android:shape="ring"
            android:thicknessRatio="8.27"
            android:useLevel="true">
            <gradient
                android:angle="270"
                android:startColor="@color/colorProgressLevelStart"
                android:endColor="@color/colorProgressLevelEnd"
                android:type="sweep" />
        </shape>
    </item>
</layer-list>
